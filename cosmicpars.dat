$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
$$ POSTFIT PARAMETERS $$$$$$$$$
s,MCMCFile=mcmctree.root;
$i,MCMCBurnIn=100;
$i,NMCEvents=500;
$i,NDataEvents=500000;
$i,NMCMCPts=20;
