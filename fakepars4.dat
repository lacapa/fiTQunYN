$s,hFactoryOutput=/nfs/data41/t2k/amissert/processed/fake4/filled_histos_wide.root;
$$ PROCESSED MC FILES
s,hFactoryMCFiles=/nfs/data41/t2k/amissert/processed/fake4/*ppmc*.root;
$$ PROCESSED DATA FILES
s,hFactoryDataFiles=/nfs/data41/t2k/amissert/processed/fake4/*ppd*.root;

$$$$$$$$$$$$$$$$$$$$$$$$$$$$
$$ HISTOGRAM BINNING $$$$$$$
i,nBinsAtt0=90;
i,nBinsDataAtt0=30;
i,nBinsAtt1=50;
i,nBinsAtt2=50;
i,nBinsAtt3=50;
i,nBinsAtt4=50;
