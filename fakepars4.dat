$ s,hFactoryOutput=/nfs/data41/t2k/amissert/processed/fake4/filled_histos_samebinbin.root;
$s:hFactoryOutput=/nfs/data41/t2k/amissert/processed/fake4/filled_histos_samebin.root;
$s:hFactoryOutput=/nfs/data41/t2k/amissert/processed/fake4/filled_histos_diffbinfine.root;
$s:hFactoryOutput=/nfs/data41/t2k/amissert/processed/fake4/filled_histos_diffbinveryfine.root;
s:hFactoryOutput=/nfs/data41/t2k/amissert/processed/fake4/filled_histos_3xbin.root;
$
$s,hFactoryOutput=/nfs/data41/t2k/amissert/processed/fake4/filled_histos_same_diffbin.root;
